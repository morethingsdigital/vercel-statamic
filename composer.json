{
    "name": "morethingsdigital/vercel-statamic",
    "description": "Statamic Addon to manage Vercel deployments inside your Statamic Website",
    "license": "MIT",
    "keywords": [
        "statamic",
        "vercel",
        "devops",
        "deployment",
        "next.js"
    ],
    "authors": [
        {
            "name": "morethingsdigital GmbH",
            "email": "hi@morethings.digital",
            "role": "Maintainer"
        },
        {
            "name": "Christoph Stockinger",
            "email": "christoph@morethings.digital",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "vercel-statamic@morethings.digital"
    },
    "autoload": {
        "psr-4": {
            "Morethingsdigital\\VercelStatamic\\": "src/"
        }
    },
    "extra": {
        "statamic": {
            "name": "Vercel Statamic",
            "description": "Vercel Statamic addon"
        },
        "laravel": {
            "providers": [
                "Morethingsdigital\\VercelStatamic\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "spatie/laravel-data": "^3.7",
        "statamic/cms": "^4.12"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
